package com.bankapp.employee.model;

public class Employee {
	
	private int empId;
	private String password;
	private String name;


	public Employee(int empId, String password, String name) {
		super();
		this.empId = empId;
		this.password = password;
		this.name = name;
	}

	public int getEmpId() {
		return empId;
	}

	public void setEmpId(int empId) {
		this.empId = empId;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public String toString() {
		return "Employee [empId=" + empId + ", password=" + password + ", name=" + name + "]";
	}

	
	}
